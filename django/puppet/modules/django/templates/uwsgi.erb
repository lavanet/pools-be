[uwsgi]
binary-path=<%= @venv %>/bin/uwsgi
pidfile=<%= @uwsgi_path %>/uwsgi.pid
chdir=<%= @path %>
module=config.wsgi:application
master=true
processes=<%= @uwsgi_processes %>
threads=<%= @uwsgi_threads %>
env=LANG='en_US.UTF-8'
socket=127.0.0.1:49152
stats=127.0.0.1:9191
max-requests=5000
harakiri=30
lazy-apps=true
